{
  "name": "eCleaner (Forget Button)",
  "short_name": "iecleaner",
  "description": "Quickly delete recent browser history, cookies, cache, downloads, forms, and passwords from a defined date",
  "author": "Andy Portmen",
  "version": "0.1.3",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "browsingData"
  ],
  "homepage_url": "http://add0n.com/ecleaner.html",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "common.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png"
    },
    "default_popup": "data/panel/index.html"
  },
  "applications": {
    "gecko": {
      "id": "{13ea5d17-aa13-474a-b8cd-891073b53c66}"
    }
  }
}
