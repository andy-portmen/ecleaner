{
  "name": "Forget Button - Clean your Browser",
  "description": "Quickly delete recent browser history, cookies, cache, downloads, forms, and passwords from a defined date",
  "version": "0.1.8.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "notifications",
    "browsingData"
  ],
  "homepage_url": "https://add0n.com/ecleaner.html",
  "icons": {
    "16": "data/icons/16.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "38": "data/icons/38.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "common.js"
    ]
  },
  "browser_action": {
    "default_popup": "data/panel/index.html"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  },
  "offline_enabled": true
}
